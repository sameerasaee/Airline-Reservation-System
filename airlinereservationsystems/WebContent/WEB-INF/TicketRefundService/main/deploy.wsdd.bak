<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TicketRefundService WSDL service -->

  <service name="TicketRefund" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://main"/>
      <parameter name="wsdlServiceElement" value="TicketRefundService"/>
      <parameter name="schemaQualified" value="http://main"/>
      <parameter name="wsdlServicePort" value="TicketRefund"/>
      <parameter name="className" value="main.TicketRefundSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TicketRefund"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="refund" qname="operNS:refund" xmlns:operNS="http://main" soapAction="" >
        <parameter qname="pns:args" xmlns:pns="http://main" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="refund"/>

  </service>
</deployment>
